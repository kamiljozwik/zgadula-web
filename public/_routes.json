{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/_assets/*",
    "/_headers",
    "/_routes.json",
    "/build/*",
    "/*.ico",
    "/*.png",
    "/*.svg",
    "/*.jpg",
    "/*.jpeg",
    "/*.gif",
    "/*.webp"
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "/index.html"
    }
  ]
}
